web-ui.pid
web-ui.log
api-server.pid
api-server.log
collab-server.pid
collab-server.log
shared-workspace-server.pid
shared-workspace-server.log

# Node modules (web-ui)
web-ui/node_modules/
web-ui/dist/
web-ui/.env
